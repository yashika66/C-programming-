#include <stdio.h>

// Function to add two numbers without arguments
int add() {
    int a, b;
    printf("Enter first number: ");
    scanf("%d", &a);
    printf("Enter second number: ");
    scanf("%d", &b);
    return a + b;  // Return the sum
}

int main() {
    int sum;

    // Call function and store result
    sum = add();

    // Display result
    printf("Sum is %d\n", sum);

    return 0;
}
output
Sum is 30