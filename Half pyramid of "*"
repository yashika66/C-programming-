#include <stdio.h>

int main() {
    int rows, i, j;

    printf("Enter the number of rows: ");
    scanf("%d", &rows);

    for (i = 1; i <= rows; i++) {
        // Inner loop for printing stars
        for (j = 1; j <= i; j++) {
            printf("* ");
        }
        printf("\n"); 
    }

    return 0;
}
output
Enter the number of rows: 5
* 
* * 
* * * 
* * * * 
* * * * * 
