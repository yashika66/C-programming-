#include <stdio.h>

int main() {
    int arr[] = {12, 45, 7, 89, 23};
    int n = sizeof(arr)/sizeof(arr[0]);
    int max = arr[0];

    for(int i = 1; i < n; i++) {
        if(arr[i] > max)
            max = arr[i];
    }

    printf("Maximum element = %d\n", max);
    return 0;
}

output
Maximum element = 89 
