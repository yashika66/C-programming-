#include <stdio.h>

int main() {
    int base, exponent;
    long long result = 1;

    printf("Enter base: ");
    scanf("%d", &base);

    printf("Enter exponent: ");
    scanf("%d", &exponent);

    int i = 0;
    while (i < exponent) {
        result *= base;
        i++;
    }

    printf("%d^%d = %lld\n", base, exponent, result);
    return 0;
}

output
Enter base: 3
Enter exponent: 3
3^3 = 27 