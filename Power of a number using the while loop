#include <stdio.h>

int main() {
    int base, exponent;
    long long result = 1;  

    printf("Enter base: ");
    scanf("%d", &base);

    printf("Enter exponent: ");
    scanf("%d", &exponent);

    int i = 1;

    while (i <= exponent) {
        result = result * base;
        i++;
    }

    printf("%d ^ %d = %lld\n", base, exponent, result);

    return 0;
}
output
Enter base: 2
Enter exponent: 5
2 ^ 5 = 32